% layout 'default';
% title 'Create Moderator';
<h2><%= title %></h2>
<form method="post">
  <div class="name field">
    <%= label_for name => 'Name' %>
    <%= text_field 'name' %>
  </div>
  <div class="email field">
    <%= label_for email => 'Email' %>
    <%= email_field 'email' %>
  </div>
  <div class="password field">
    <%= label_for password => 'Password' %>
    <%= password_field 'password' %>
  </div>
  <%= submit_button 'Create' %>
</form>
